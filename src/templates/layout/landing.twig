<!DOCTYPE html>
<html lang="ru">

  <head>

    <meta
    charset="utf-8">
    <!-- <base href="/"> -->
    <title>{{ page_title }}</title>
    <meta name="description" content="">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta
    name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, viewport-fit=cover">


    <!-- Template Basic Images Start -->
    <meta property="og:image" content="path/to/image.jpg">
    <link rel="icon" href="#">
    <link rel="apple-touch-icon" sizes="180x180" href="#">
    <!-- Template Basic Images End -->

    <!-- Custom Browsers Color Start -->
  <meta
    name="theme-color" content="#4489ff"> <!-- Custom Browsers Color End -->
  </head>

  <body>
    <main>
      {% block header %}
        {% include "../components/header/header.twig" %}
      {% endblock %}
      {% block body %}
        {% block mainScreen %}
          {% include "../components/screen/screen__landing.twig" %}
          {% include "../components/slider/slider-bg.twig" %}
        {% endblock %}
      </main>
      {% block numeric %}
        {% include "../components/bullets/bullets-numeric.twig" %}
      {% endblock %}
      {% block celebrities1p1 %}{% endblock %}
      {% block celebritiesLots %}{% endblock %}
      {% block categories %}
        {% include "../components/categories/categories-landing.twig" %}
      {% endblock %}
      {% block sliderReviews %}
        {% include "../components/slider/slider-reviews-landing.twig" %}
      {% endblock %}
      {% block news %}
        {% include "../components/news/news-landing.twig" %}
      {% endblock %}
      {% block sliderPartners %}
        {% include "../components/slider/slider-partners.twig" %}
      {% endblock %}
    {% endblock %}
    {% block blockchain %}
      {% include "../components/blockchain/blockchain.twig" %}
    {% endblock %}
    {% block footerMain %}
      {% block footer %}
        {% include "../components/footer/footer.twig" %}
      {% endblock %}
      {% block mobileMenu %}
        {% include "../components/mobile-menu/mobile-menu.twig" %}
      {% endblock %}
      {% block popup %}
        {% include "../components/popup/popup.twig" %}
        {% include "../components/popup/popup-sucess.twig" %}
        {% include "../components/popup/sell-item-sucess.twig" %}
      {% endblock %}
      {% block video %}
        {% include "../components/video/video-landing.twig" %}
      {% endblock %}
    {% endblock %}
  </body>

</html>
